{
    "env": {
        "commonjs": true,
        "jest": true,
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended", "plugin:prettier/recommended"],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
    }
}